Xft.dpi: 96
Xft.autohint: 0
Xft.lcdfilter:  lcddefault
Xft.hintstyle:  hintfull
Xft.hinting: 1
Xft.antialias: 1
Xft.rgba: none

URxvt.font: xft:Hermit:size=9
URxvt.boldFont: xft:Hermit:size=9
URxvt.italicFont: xft:Hermit:size=9
URxvt.boldItalicFont: xft:Hermit:size=9

URxvt.geometry: 120x24

URxvt.letterSpace: 0
URxvt.saveLines: 1000

URxvt.scrollBar: false
URxvt.scrollstyle: plain
URxvt.scrollBar_right: true
URxvt.scrollColor: #657B83

URxvt.cursorUnderline: false
URxvt.cursorBlink: false
URxvt.cursorColor: #3971ED

URxvt.scrollTtyOutput: false
URxvt.scrollWithBuffer: true
URxvt.scrollTtyKeypress: true

URxvt.cutchars: ()*,<>[]{}|`""''
URxvt.perl-ext-common: default,tabbedex,matcher,selection-to-clipboard,searchable-scrollback,resize-font

URxvt.url-launcher: /usr/bin/xdg-open
URxvt.matcher.button: 1
URxvt.matcher.pattern.1: \\bwww\\.[\\w-]+\\.[\\w./?&@#-]*[\\w/-]
URxvt.matcher.pattern.2: \\bhttps?:\/\/[\\w-.]*\/[\\w./?&@#-]*.(jpg|jpeg|png|webp)
URxvt.matcher.launcher.2: feh $0
URxvt.colorUL: 12

URxvt.tabbed.autohide: true
URxvt.tabbed.transparent: false
URxvt.tabbed.new-button: false
URxvt.tabbed.title: true
URxvt.tabbed.title-fg: 15
URxvt.tabbed.title-bg: 240
URxvt.tabbed.tabbar-fg: 15
URxvt.tabbed.tabbar-bg: 240
URxvt.tabbed.tab-fg: 232
URxvt.tabbed.tab-bg: 15


